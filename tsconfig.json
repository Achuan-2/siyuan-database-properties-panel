{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
    /* Bundler mode */
    "moduleResolution": "Node",
    // "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    /* Linting */
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    /* Svelte */
    /**
     * Typecheck JS in `.svelte` and `.js` files by default.
     * Disable checkJs if you'd like to use dynamic types in JS.
     * Note that setting allowJs false does not prevent the use
     * of JS in `.svelte` files.
    */
    "allowJs": true,
    "checkJs": true,
    "types": [
      "node",
      "vite/client",
      "svelte",
      "vitest/globals",
      "@testing-library/jest-dom",
      "./src/types/siyuan.types.ts"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/libs/*": [
        "./src/libs/*"
      ],
      "@siyuan/app/*": [
        "node_modules/siyuan-app/app/src/*"
      ]
    }
  },
  "include": [
    "tools/**/*.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.svelte",
    "node_modules/siyuan/**/*.d.ts",
    "node_modules/.pnpm/siyuan#738a9074cfaad9db08fab3b23858d025801e3120@https+++codeload.github.com+Macavity+siyuan+t_yokw3chlgvnmvnvon6w6ghufgy/node_modules/siyuan#738a9074cfaad9db08fab3b23858d025801e3120/app/src/types/index.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "root": "."
}